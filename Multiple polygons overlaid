import turtle
from turtle import Turtle,Screen
import random
turtle.colormode(255)
timmy = Turtle()
timmy.shape("arrow")
timmy.color("SeaGreen")

sides = 3

while sides < 11 :
    turn_angle = 360 / sides
    for i in range(sides) :
        timmy.forward(100)
        timmy.right(turn_angle)
    sides += 1
